{
    "[dart]": {
      "editor.tabSize": 2,
      "editor.insertSpaces": true,
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "Dart-Code.dart-code",
      "editor.rulers": [ 80 ],
      "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "always"
      },
    },
  }
  